{
  "description": "VSCode Light Theme",
  "target": "themes/light.json",
  "variant": "light",
  "content": {
    "name": "Luma",
    "colors": {
      "activityBar.background": "{{white:dark}}",
      "activityBar.foreground": "{{black:normal}}",
      "activityBarBadge.background": "{{cyan:normal}}",
      "badge.background": "{{cyan:normal;alpha:60}}",
      "banner.background": "{{white:bright}}",
      "button.background": "{{cyan:normal}}",
      "commandCenter.background": "{{white:bright}}",
      "diffEditor.insertedTextBackground": "{{green:normal;alpha:20}}",
      "diffEditor.insertedLineBackground": "{{green:normal;alpha:10}}",
      "diffEditor.removedTextBackground": "{{red:normal;alpha:10}}",
      "diffEditor.removedLineBackground": "{{red:normal;alpha:20}}",
      "diffEditorGutter.insertedLineBackground": "{{green:normal;alpha:20}}",
      "diffEditorGutter.removedLineBackground": "{{red:normal;alpha:30}}",
      "diffEditorOverview.insertedForeground": "{{green:normal;alpha:70}}",
      "diffEditorOverview.removedForeground": "{{red:normal;alpha:70}}",
      "dropdown.background": "{{white:bright}}",
      "editor.background": "{{white:normal}}",
      "editor.findMatchBackground": "{{cyan:normal;alpha:30}}",
      "editor.findMatchHighlightBackground": "{{orange:normal;alpha:40}}",
      "editor.foreground": "{{black:normal}}",
      "editor.lineHighlightBackground": "{{white:dark}}",
      "editor.selectionBackground": "{{cyan:normal;alpha:30}}",
      "editor.wordHighlightBackground": "{{cyan:normal;alpha:30}}",
      "editor.wordHighlightStrongBackground": "{{cyan:normal;alpha:30}}",
      "editorBracketHighlight.foreground1": "{{cyan:normal}}",
      "editorBracketHighlight.foreground2": "{{orange:normal}}",
      "editorBracketHighlight.foreground3": "{{blue:normal}}",
      "editorBracketHighlight.foreground4": "{{yellow:normal}}",
      "editorBracketHighlight.foreground5": "{{green:normal}}",
      "editorBracketHighlight.foreground6": "{{magenta:normal}}",
      "editorBracketHighlight.unexpectedBracket.foreground": "{{red:normal}}",
      "editorGroupHeader.noTabsBackground": "{{white:normal}}",
      "editorGroupHeader.tabsBackground": "{{white:normal}}",
      "editorGutter.background": "{{white:normal}}",
      "editorInlayHint.background": "{{white:normal}}",
      "editorInlayHint.foreground": "{{cyan:normal;alpha:50}}",
      "editorLineNumber.foreground": "{{black:normal;alpha:40}}",
      "editorLineNumber.activeForeground": "{{black:normal}}",
      "gitlens.trailingLineForegroundColor": "{{black:normal;alpha:40}}",
      "input.background": "{{white:bright}}",
      "list.activeSelectionBackground": "{{cyan:normal;alpha:30}}",
      "list.activeSelectionForeground": "{{black:normal}}",
      "list.dropBackground": "{{white:normal}}",
      "list.inactiveSelectionBackground": "{{cyan:normal;alpha:30}}",
      "list.inactiveSelectionForeground": "{{black:normal}}",
      "menu.background": "{{white:normal}}",
      "panel.background": "{{white:normal}}",
      "quickInput.background": "{{white:normal}}",
      "quickInput.foreground": "{{black:normal}}",
      "scrollbarSlider.background": "{{black:normal;alpha:30}}",
      "searchEditor.findMatchBackground": "{{cyan:normal;alpha:30}}",
      "selection.background": "{{cyan:normal;alpha:30}}",
      "sideBar.background": "{{white:dark}}",
      "sideBarSectionHeader.background": "{{white:dark}}",
      "statusBar.background": "{{white:dark}}",
      "statusBar.debuggingBackground": "{{orange:normal;alpha:80}}",
      "statusBar.foreground": "{{black:normal}}",
      "tab.activeBackground": "{{white:normal}}",
      "tab.inactiveBackground": "{{white:dark}}",
      "tab.border": "{{white:dark}}",
      "tab.selectedBorderTop": "{{white:dark}}",
      "terminal.background": "{{white:normal}}",
      "terminal.ansiBrightBlack": "{{black:bright}}",
      "terminal.ansiBrightBlue": "{{blue:bright}}",
      "terminal.ansiBrightCyan": "{{cyan:bright}}",
      "terminal.ansiBrightGreen": "{{green:bright}}",
      "terminal.ansiBrightMagenta": "{{magenta:bright}}",
      "terminal.ansiBrightRed": "{{red:bright}}",
      "terminal.ansiBrightYellow": "{{yellow:bright}}",
      "terminal.ansiBlack": "{{black:normal}}",
      "terminal.ansiBlue": "{{blue:normal}}",
      "terminal.ansiCyan": "{{cyan:normal}}",
      "terminal.ansiGreen": "{{green:normal}}",
      "terminal.ansiMagenta": "{{magenta:normal}}",
      "terminal.ansiRed": "{{red:normal}}",
      "terminal.ansiYellow": "{{yellow:normal}}",
      "terminal.foreground": "{{black:normal}}",
      "titleBar.activeBackground": "{{white:dark}}",
      "titleBar.activeForeground": "{{black:normal}}",
      "titleBar.inactiveBackground": "{{white:dark}}",
      "titleBar.inactiveForeground": "{{black:normal}}"
    },
    "tokenColors": [
      {
        "name": "Comments",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "Comments: Preprocessor",
        "scope": "comment.block.preprocessor",
        "settings": {
          "fontStyle": "",
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "Comments: Documentation",
        "scope": ["comment.documentation", "comment.block.documentation"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "Invalid - Illegal",
        "scope": "invalid.illegal",
        "settings": {
          "foreground": "{{red:normal}}"
        }
      },
      {
        "name": "Operators",
        "scope": "keyword.operator",
        "settings": {
          "fontStyle": "",
          "foreground": "{{red:normal}}"
        }
      },
      {
        "name": "Operators",
        "scope": [
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.operator.assignment",
          "punctuation",
          "storage.type.function.arrow",
          "meta.brace"
        ],
        "settings": {
          "fontStyle": "",
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Keywords",
        "scope": [
          "keyword",
          "storage",
          "keyword.operator.new",
          "storage.type",
          "keyword.operator.expression",
          "support.constant",
          "variable.language",
          "support.type"
        ],
        "settings": {
          "fontStyle": "",
          "foreground": "{{yellow:normal}}"
        }
      },
      {
        "name": "Variables",
        "scope": ["variable", "support.variable"],
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Language Constants",
        "scope": ["constant.language", "support.type.builtin.ts"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{magenta:normal}}"
        }
      },
      {
        "name": "Functions",
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "fontStyle": "",
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "Classes",
        "scope": [
          "entity.name.type",
          "entity.other.inherited-class",
          "support.class"
        ],
        "settings": {
          "fontStyle": "",
          "foreground": "{{blue:normal}}"
        }
      },
      {
        "name": "Exceptions",
        "scope": "entity.name.exception",
        "settings": {
          "foreground": "{{blue:normal}}"
        }
      },
      {
        "name": "Sections",
        "scope": "entity.name.section",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Characters",
        "scope": ["constant.numeric", "constant.character", "constant"],
        "settings": {
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "Numbers",
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "{{pink:normal}}"
        }
      },
      {
        "name": "Strings",
        "scope": "string",
        "settings": {
          "foreground": "{{cyan:normal}}"
        }
      },
      {
        "name": "Strings: Escape Sequences",
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "{{pink:normal}}"
        }
      },
      {
        "name": "Strings: Regular Expressions",
        "scope": [
          "string.regexp",
          "constant.other.character-class.range.regexp"
        ],
        "settings": {
          "foreground": "{{magenta:normal}}"
        }
      },
      {
        "name": "Strings: Symbols",
        "scope": "constant.other.symbol",
        "settings": {
          "foreground": "{{cyan:normal}}"
        }
      },
      {
        "name": "HTML: Doctype Declaration",
        "scope": [
          "meta.tag.sgml.doctype",
          "meta.tag.sgml.doctype string",
          "meta.tag.sgml.doctype entity.name.tag",
          "meta.tag.sgml punctuation.definition.tag.html"
        ],
        "settings": {
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "HTML: Tags",
        "scope": [
          "meta.tag",
          "punctuation.definition.tag.html",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "HTML: Tag Names",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "{{magenta:normal}}"
        }
      },
      {
        "name": "HTML: Attribute Names",
        "scope": [
          "meta.tag entity.other.attribute-name",
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{blue:normal}}"
        }
      },
      {
        "name": "HTML: Entities",
        "scope": ["constant.character.entity", "punctuation.definition.entity"],
        "settings": {
          "foreground": "{{yellow:normal}}"
        }
      },
      {
        "name": "CSS: Selectors",
        "scope": [
          "meta.selector",
          "meta.selector entity",
          "meta.selector entity punctuation",
          "entity.name.tag.css"
        ],
        "settings": {
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "CSS: Property Names",
        "scope": ["meta.property-name", "support.type.property-name"],
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "CSS: Property Values",
        "scope": [
          "meta.property-value",
          "meta.property-value constant.other",
          "support.constant.property-value"
        ],
        "settings": {
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "CSS: Important Keyword",
        "scope": "keyword.other.important",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup: Changed",
        "scope": "markup.changed",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Deletion",
        "scope": "markup.deleted",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Emphasis",
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup: Error",
        "scope": "markup.error",
        "settings": {
          "foreground": "{{red:normal}}"
        }
      },
      {
        "name": "Markup: Insertion",
        "scope": "markup.inserted",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Link",
        "scope": "meta.link",
        "settings": {
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "Markup: Output",
        "scope": ["markup.output", "markup.raw"],
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Prompt",
        "scope": "markup.prompt",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Heading",
        "scope": "markup.heading",
        "settings": {
          "foreground": "{{green:normal}}"
        }
      },
      {
        "name": "Markup: Strong",
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup: Traceback",
        "scope": "markup.traceback",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup: Underline",
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "name": "Markup Quote",
        "scope": "markup.quote",
        "settings": {
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "Markup Lists",
        "scope": "markup.list",
        "settings": {
          "foreground": "{{black:normal}}"
        }
      },
      {
        "name": "Markup Styling",
        "scope": ["markup.bold", "markup.italic"],
        "settings": {
          "foreground": "{{magenta:normal}}"
        }
      },
      {
        "name": "Markup Inline",
        "scope": "markup.inline.raw",
        "settings": {
          "fontStyle": "",
          "foreground": "{{orange:normal}}"
        }
      },
      {
        "name": "Extra: Diff Range",
        "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
        "settings": {
          "foreground": "{{black:bright}}"
        }
      },
      {
        "name": "Extra: Diff From",
        "scope": "meta.diff.header.from-file",
        "settings": {
          "foreground": "{{black:bright}}"
        }
      },
      {
        "name": "Extra: Diff To",
        "scope": "meta.diff.header.to-file",
        "settings": {
          "foreground": "{{black:bright}}"
        }
      }
    ]
  }
}
