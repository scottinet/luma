    "titleBar.inactiveForeground": "#2f413a"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#009f42"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#009f42"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#009f42"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#f2004e"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "fontStyle": "",
        "foreground": "#f2004e"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "punctuation",
        "storage.type.function.arrow",
        "meta.brace"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator.new",
        "storage.type",
        "keyword.operator.expression",
        "support.constant",
        "variable.language",
        "support.type"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a37f00"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.type.builtin.ts"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#885fff"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#1a7dff"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#1a7dff"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#009f42"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e300a6"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#0094a7"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#CB3A9D"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": ["string.regexp", "constant.other.character-class.range.regexp"],
      "settings": {
        "foreground": "#885fff"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#0094a7"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#009f42"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#885fff"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1a7dff"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#a37f00"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#f2004e"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#009f42"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#2f413a"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#885fff"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#cf5f00"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    }
  ]
}
